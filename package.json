{
  "name": "viktoriya-gonch",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --profile",
    "watch": "ng build --watch --configuration development",
    "seo:build": "ng build --configuration production && npx scully --scanRoutes --project viktoriya-gonch",
    "seo:serve": "npm run seo:build && npx scully serve --project viktoriya-gonch",
    "deploy": "npx angular-cli-ghpages --dir=dist/static",
    "deploy:ftp": "node deploy.js",
    "update": "npm run seo:build && npm run deploy:ftp"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.0.0",
    "@angular/cdk": "^18.2.14",
    "@angular/common": "^18.0.0",
    "@angular/compiler": "^18.0.0",
    "@angular/core": "^18.0.0",
    "@angular/forms": "^18.0.0",
    "@angular/material": "^18.2.14",
    "@angular/platform-browser": "^18.0.0",
    "@angular/platform-browser-dynamic": "^18.0.0",
    "@angular/router": "^18.0.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@scullyio/ng-lib": "^2.1.41",
    "@scullyio/scully": "^2.1.41",
    "@scullyio/scully-plugin-puppeteer": "^2.1.41",
    "@swimlane/ngx-charts": "^21.0.0",
    "@types/swiper": "^6.0.0",
    "basic-ftp": "^5.0.5",
    "dotenv": "^16.4.7",
    "ngx-owl-carousel-o": "^18.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.0.3",
